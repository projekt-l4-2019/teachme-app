<?xml version='1.0' encoding='utf-8'?>
<widget id="com.pgp.teachme" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TeachMe</name>
    <description>
        TeachMe - find your coach!
    </description>
    <author email="pgp2019l4@gmail.com" href="http://projekt-l4-2019.github.io">
        Polska Grupa Programistyczna L4
    </author>
    <content src="index.html" />
    <icon src="res/logo.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
